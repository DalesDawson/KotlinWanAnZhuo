<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e006.release">
    <!-- if you want to load images from the internet -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- if you want to load images from a file OR from the internet -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.e006.com.e006.release.App"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light.NoActionBar">
        <activity android:name="com.e006.com.e006.release.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.e006.com.e006.release.activity.MainActivity" />
        <activity android:name="com.e006.com.e006.release.activity.CommonWebViewActivity" />
        <activity android:name="com.e006.com.e006.release.activity.SystemListActivity" />
        <activity android:name="com.e006.com.e006.release.activity.SearchListActivity" />
        <activity android:name="com.e006.com.e006.release.activity.LoginActivity" />
        <activity android:name="com.e006.com.e006.release.activity.RegisterActivity" />
        <activity android:name="com.e006.com.e006.release.activity.CollectionActivity" />
    </application>

</manifest>